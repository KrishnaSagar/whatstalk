{
    "manifest_version": 2,
    "name": "whatstalk",
    "version": "0.1",
    "description": "Track WhatsApp status",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["https://web.whatsapp.com/"],
        "js": ["js/inject.js"]
    }],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "author": "Mohamad Radisha <pr0ph0z23@gmail.com>"
}